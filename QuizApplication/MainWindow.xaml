<Window x:Class="QuizApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizApplication"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnFaculty" Content="Faculty" HorizontalAlignment="Left" Margin="100,132,0,0" VerticalAlignment="Top" Width="94" Click="btnFaculty_Click"/>
        <Label x:Name="lblFaculty" Content="Login as Faculty" HorizontalAlignment="Left" Margin="100,185,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblStudent" Content="Login as Student" HorizontalAlignment="Left" Margin="321,185,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnStudent" Content="Student" HorizontalAlignment="Left" Margin="321,132,0,0" VerticalAlignment="Top" Width="98" Click="btnStudent_Click"/>

    </Grid>
</Window>
